<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Boundary.CoachProfile">
    <children>
        <TabPane prefHeight="700.0" prefWidth="1000.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab style="-fx-background-color: #87c9ff; -fx-font-family: &quot;Futura&quot;;" text="Personal Info">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="658.0" prefWidth="1000.0" styleClass="bg-color" stylesheets="@style.css">
                            <children>
                                <VBox alignment="CENTER" layoutX="97.0" layoutY="83.0" prefHeight="438.0" prefWidth="291.0">
                                    <children>
                                        <HBox alignment="CENTER" prefHeight="89.0" prefWidth="248.0" spacing="20.0">
                                            <children>
                                                <Label prefHeight="17.0" prefWidth="44.0" styleClass="label-holders" stylesheets="@Profile.css" text="ID" />
                                                <Label fx:id="IDHolder" styleClass="info-holders" stylesheets="@Profile.css" text="(trainerID)" />
                                            </children>
                                        </HBox>
                                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                                            <children>
                                                <Label prefHeight="17.0" prefWidth="62.0" styleClass="label-holders" stylesheets="@Profile.css" text="User Name" />
                                                <TextField fx:id="nameInput">
                                                    <stylesheets>
                                                        <URL value="@Profile.css" />
                                                        <URL value="@style.css" />
                                                    </stylesheets>
                                                    <styleClass>
                                                        <String fx:value="info-holders" />
                                                        <String fx:value="transparent" />
                                                    </styleClass>
                                                </TextField>
                                            </children>
                                        </HBox>
                                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                                            <children>
                                                <Label styleClass="label-holders" stylesheets="@Profile.css" text="Height (cm)" />
                                                <TextField fx:id="heightInput">
                                                    <stylesheets>
                                                        <URL value="@Profile.css" />
                                                        <URL value="@style.css" />
                                                    </stylesheets>
                                                    <styleClass>
                                                        <String fx:value="info-holders" />
                                                        <String fx:value="transparent" />
                                                    </styleClass>
                                                </TextField>
                                            </children>
                                        </HBox>
                                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                                            <children>
                                                <Label prefHeight="18.0" prefWidth="76.0" styleClass="label-holders" stylesheets="@Profile.css" text="Weight(kG)" />
                                                <TextField fx:id="weightInput">
                                                    <stylesheets>
                                                        <URL value="@Profile.css" />
                                                        <URL value="@style.css" />
                                                    </stylesheets>
                                                    <styleClass>
                                                        <String fx:value="info-holders" />
                                                        <String fx:value="transparent" />
                                                    </styleClass>
                                                </TextField>
                                            </children>
                                        </HBox>
                                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                                            <children>
                                                <Label styleClass="label-holders" stylesheets="@Profile.css" text="Gender" />
                                                <ChoiceBox fx:id="genderSelection" prefHeight="26.0" prefWidth="115.0" stylesheets="@Profile.css" />
                                            </children>
                                        </HBox>
                              <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                                  <children>
                                      <Label prefHeight="18.0" prefWidth="76.0" styleClass="label-holders" stylesheets="@Profile.css" text="Phone" />
                                      <TextField fx:id="Phone">
                                          <stylesheets>
                                              <URL value="@Profile.css" />
                                              <URL value="@style.css" />
                                          </stylesheets>
                                          <styleClass>
                                              <String fx:value="info-holders" />
                                              <String fx:value="transparent" />
                                          </styleClass>
                                      </TextField>
                                  </children>
                              </HBox>
                                    </children>
                                </VBox>
                                <Button fx:id="saveBtn" layoutX="175.0" layoutY="557.0" mnemonicParsing="false" onAction="#onSaveBtnClicked" styleClass="saveButton" stylesheets="@Profile.css" text="Save" textFill="WHITE">
                                    <font>
                                        <Font size="19.0" />
                                    </font>
                                </Button>
                                <Text fx:id="mainPage" fill="#0018f5" layoutX="759.0" layoutY="601.0" onMouseClicked="#toMainPage" strokeType="OUTSIDE" strokeWidth="0.0" text="Back to main page" underline="true" />
                        <Label layoutX="471.0" layoutY="101.0" prefHeight="59.0" prefWidth="261.0" text="Self Introduction ">
                           <font>
                              <Font size="28.0" />
                           </font></Label>
                        <TextArea fx:id="CoachIntro" layoutX="484.0" layoutY="174.0" prefHeight="192.0" prefWidth="201.0" />
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
