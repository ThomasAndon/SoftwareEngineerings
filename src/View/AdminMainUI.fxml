<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="window" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Boundary.AdminMain">
    <children>
        <Button fx:id="userInfo" layoutX="184.0" layoutY="62.0" mnemonicParsing="false" onAction="#toUserInfo" prefHeight="45.0" prefWidth="207.0" text="UserInfo" />
        <Button fx:id="coachInfo" layoutX="184.0" layoutY="126.0" mnemonicParsing="false" onAction="#toCoachInfo" prefHeight="45.0" prefWidth="207.0" text="CoachInfo" />
        <Button fx:id="sessionInfo" layoutX="184.0" layoutY="184.0" mnemonicParsing="false" onAction="#toSessionInfo" prefHeight="45.0" prefWidth="207.0" text="SessionInfo" />
        <Button fx:id="edit" layoutX="184.0" layoutY="240.0" mnemonicParsing="false" onAction="#editAd" prefHeight="45.0" prefWidth="207.0" text="EditAdvertisement" />
        <Text fx:id="Exit" fill="#007bff" layoutX="468.0" layoutY="369.0" strokeType="OUTSIDE" onMouseClicked="#toLogin" strokeWidth="0.0" text="Exit" underline="true" />
        <Label layoutX="87.0" layoutY="8.0" prefHeight="54.0" prefWidth="401.0" text="Welcome Admin">
            <font>
                <Font size="42.0" />
            </font>
        </Label>
      <Button fx:id="uploadVideo" layoutX="184.0" layoutY="296.0" mnemonicParsing="false" onAction="#onUploadVideoClicked" prefHeight="45.0" prefWidth="207.0" text="Upload Video" />
    </children>
</AnchorPane>
