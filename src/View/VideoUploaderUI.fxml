<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Boundary.VideoUploader">
   <children>
      <TableView fx:id="videoTable" layoutX="70.0" layoutY="110.0" prefHeight="445.0" prefWidth="575.0">
        <columns>
          <TableColumn fx:id="titleCol" editable="false" prefWidth="404.0" resizable="false" text="Title" />
          <TableColumn fx:id="typeCol" minWidth="4.0" prefWidth="170.0" resizable="false" text="Type" />
        </columns>
      </TableView>
      <VBox alignment="CENTER" layoutX="686.0" layoutY="264.0" prefHeight="138.0" prefWidth="216.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onOpenClicked" prefHeight="26.0" prefWidth="110.0" text="Open" />
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onDeletedSelected" prefHeight="26.0" prefWidth="110.0" text="Delete selected" />
               </children>
            </VBox>
         </children>
      </VBox>
      <HBox alignment="CENTER" layoutX="70.0" layoutY="566.0" prefHeight="47.0" prefWidth="575.0">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="47.0" prefWidth="244.0">
               <children>
                  <TextField fx:id="titleInput" prefHeight="26.0" prefWidth="212.0" promptText="Title" />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="47.0" prefWidth="102.0">
               <children>
                  <Button fx:id="pathBtn" mnemonicParsing="false" onAction="#onChoosePathClicked" prefHeight="26.0" prefWidth="78.0" text="Path..." />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="47.0" prefWidth="46.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Type:" />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="47.0" prefWidth="139.0">
               <children>
                  <ChoiceBox fx:id="typeChoice" prefWidth="150.0" />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onAddBtnClicked" text="Add" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
